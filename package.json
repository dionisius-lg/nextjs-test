{
  "name": "pos",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8080",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.0.0",
    "@hookform/resolvers": "^2.8.0",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.1",
    "file-saver": "^2.0.5",
    "highcharts": "^9.3.3",
    "highcharts-react-official": "^3.1.0",
    "interweave": "^12.9.0",
    "iron-session": "^6.0.5",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "next": "12.0.10",
    "nextjs-progressbar": "^0.0.13",
    "node-fetch": "^3.2.0",
    "prop-types": "^15.8.1",
    "react": "17.0.2",
    "react-bootstrap": "^1.6.4",
    "react-datepicker": "^3.2.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.26.1",
    "react-select": "^5.2.2",
    "swr": "^1.2.1",
    "with-query": "^1.3.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2019,
      "sourceType": "module"
    },
    "settings": {
      "react": {
        "version": "16.3.0"
      }
    },
    "plugins": [
      "react",
      "react-hooks"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": "warn",
      "react-hooks/rules-of-hooks": "error",
      "react/no-unescaped-entities": "off",
      "react/prop-types": "warn"
    },
    "ignorePatterns": [
      "node_modules/"
    ]
  }
}
